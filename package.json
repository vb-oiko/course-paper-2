{
  "name": "course-paper-2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-ts": "tsc",
    "build": "npm run build-ts && npm run lint",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "serve-debug": "nodemon --inspect dist/index.js",
    "watch-ts": "tsc -w",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "nodemon dist/index.js",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "migrate:up": "tsc && node ./dist/db/migrate.js up",
    "migrate:down": "tsc && node ./dist/db/migrate.js down",
    "seed": "tsc && node ./dist/db/seed.js",
    "start": "tsc && node ./dist/index.js"
  },
  "author": "Vasyl Boyko",
  "license": "ISC",
  "devDependencies": {
    "@types/faker": "^5.5.7",
    "@types/node": "^16.3.2",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "concurrently": "^6.2.0",
    "eslint": "^7.30.0",
    "nodemon": "^2.0.12",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "faker": "^5.5.3",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "sql-template-tag": "^4.0.0"
  }
}
